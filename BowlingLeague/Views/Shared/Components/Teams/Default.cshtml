@{
}

@model IEnumerable<string>

<h4>Filter by Team</h4>

<a asp-controller="Home" asp-action="Index" asp-route-TeamName="" class="btn btn-block my-create">Home</a>

@foreach (string t in Model)
{
    <a asp-controller="Home" asp-action="Index" asp-route-TeamName="@t"
       class="btn btn-block @(ViewBag.SelectedTeam == t ? "my-filled" : "my-create")">@t</a>
}